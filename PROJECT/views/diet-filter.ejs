            { value: 'Cholesterol Control', label: 'Cholesterol Control' }
          ]
        },
        'womens-health': {
          title: 'Women\'s Health Dietitians',
          heading: 'Find a Women\'s Health Nutritionist',
          script: '/js/womens_health.js',
          specializations: [
            { value: 'PCOS', label: 'PCOS' },
            { value: 'Pregnancy Nutrition', label: 'Pregnancy Nutrition' },
            { value: 'Menopause', label: 'Menopause' },
            { value: 'Fertility', label: 'Fertility' }
          ]
        }
      };

      function loadPlanContent() {
        const urlParams = new URLSearchParams(window.location.search);
        const planName = urlParams.get('plan') || 'diabetes-thyroid';
        const plan = plans[planName] || plans['diabetes-thyroid'];

        // Update page title
        document.getElementById('pageTitle').textContent = plan.title;

        // Update main heading
        document.getElementById('mainHeading').textContent = plan.heading;

        // Update navbar active state
        document.querySelectorAll('.nav-item').forEach(item => item.classList.remove('active-page'));
        const navItem = document.getElementById(`nav-${planName}`);
        if (navItem) navItem.parentElement.classList.add('active-page');

        // Update specialization filters
        const filterContainer = document.getElementById('specializationFilters');
        filterContainer.innerHTML = plan.specializations.map(spec => `
          <label class="checkbox-item">
            <input type="checkbox" name="specialization" value="${spec.value}" onchange="applyFilters()"> ${spec.label}
          </label>
        `).join('');

        // Load plan-specific script
        const existingScript = document.querySelector('script[data-plan-script]');
        if (existingScript) existingScript.remove();
        const script = document.createElement('script');
        script.src = plan.script;
        script.dataset.planScript = true;
        document.body.appendChild(script);
      }

      window.onload = loadPlanContent;
    </script>
    <script src="/js/dietitian_profiles.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
  </body>
