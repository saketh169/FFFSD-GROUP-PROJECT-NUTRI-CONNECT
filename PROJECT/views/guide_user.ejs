     <style>
      body {
        margin: 0;
        padding: 0;
        background-color: #e8f5e9;
        display: flex;
        justify-content: center;
        flex-direction: column;
        align-items: center;
        font-family: Arial, sans-serif;
      }
      
      /* User Introduction Section */
      .user-intro {
        display: flex;
        justify-content: center;
        flex-wrap: wrap;
        height: auto;
        margin: 60px;
        border: 2px solid green;
        border-radius: 10px;
        width: 90%;
        background-color: rgba(60, 239, 60, 0.132);
        padding: 20px;
        transition: transform 0.3s ease-in-out;
      }
      
      /* Hover Effect */
      .user-intro:hover {
        transform: scale(1.02);
        box-shadow: 0px 0px 20px rgba(0, 128, 0, 0.2);
      }
      
      /* Logo Styling */
      .website-logo-guide {
        display: flex;
        justify-content: center;
        align-items: center;
        width: 100%;
        margin-top: 30px;
      }
      
      .website-logo-guide img {
        height: 120px;
        display: flex;
        justify-content: center;
        align-items: center;
        border-radius: 60px;
        transition: transform 0.3s ease-in-out;
      }
      
      /* Logo Hover Effect */
      .website-logo-guide img:hover {
        transform: scale(1.1);
        box-shadow: 0px 0px 15px rgba(0, 128, 0, 0.3);
      }
      
      /* Section Headings */
      .user-intro-heading,
      .user-intro-paragraph,
      .other-features-heading {
        display: flex;
        justify-content: center;
        align-items: center;
        text-align: center;
        margin: 10px;
        padding: 10px;
        font-size: 24px;
        font-weight: bold;
        color: green;
      }
      
      /* Paragraph Styling */
      .user-intro-paragraph {
        font-size: 20px;
        line-height: 1.5;
        color: #333;
      }
      
      /* Steps and Features Containers */
      .steps-container,
      .other-features-container {
        height: auto;
        display: flex;
        flex-direction: column;
        gap: 20px;
        width: 90%;
        margin: 60px;
      }
      
      /* Common Styles for Steps and Features */
      .steps, .features {
        display: flex;
        justify-content: space-between;
        align-items: center;
        border: 2px solid #ff9800;  /* Orange border to match nutrition theme */
        height: 400px;
        width: 100%;
        background: linear-gradient(to right, #fff3e0, #ffecb3); /* Soft orange-yellow */
        border-radius: 10px;
        transition: all 0.3s ease-in-out;
        padding: 20px;
        position: relative;
      }
      
      @media (max-width: 992px) {
        .steps, .features {
          flex-direction: column;
          height: auto;
          text-align: center;
        }
        .steps-img, .features-img {
          width: 80%;
          height: auto;
        }
      }
      
      /* Hover Effect */
      .steps:hover, .features:hover {
        transform: scale(1.02);
        box-shadow: 0px 0px 25px rgba(255, 152, 0, 0.4);
      }
      
      /* Alternate Image Positioning */
      .steps:nth-child(even),
      .features:nth-child(even) {
        flex-direction: row;
        text-align: right;
        background: linear-gradient(to right, #e8f5e9, #c8e6c9); /* Soft green gradient */
        border-color: #388e3c; /* Dark green border */
      }
      
      .steps:nth-child(odd),
      .features:nth-child(odd) {
        flex-direction: row-reverse;
        text-align: left;
        background: linear-gradient(to right, #fff3e0, #ffecb3); /* Soft orange-yellow */
        border-color: #ff9800; /* Orange border */
      }
      
      /* Responsive Layout Fix */
      @media (max-width: 992px) {
        .steps:nth-child(even), .features:nth-child(even),
        .steps:nth-child(odd), .features:nth-child(odd) {
          flex-direction: column;
          text-align: center;
        }
      }
      
      /* Text Container */
      .steps-info, .features-info {
        flex: 1;
        text-align: center;
        font-size: 18px;
        padding: 20px;
        color: #4e342e;
        font-weight: bold;
      }
      
      /* Image Styling */
      .steps-img, .features-img {
        height: 90%;
        width: 50%;
        border-radius: 5px;
        transition: transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out;
      }
      
      /* Image Hover Effect */
      .steps-img:hover, .features-img:hover {
        transform: scale(1.05);
        box-shadow: 0px 0px 20px rgba(76, 175, 80, 0.4);
      }
      
          
     </style>

