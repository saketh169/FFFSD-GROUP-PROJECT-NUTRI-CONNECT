    <script>
      // Sample dietitian data with updated specializations
      const dietitians = [
        {
          Name: "Dr Anna Williams",
          specialization: "weightLossGain",
          bookedDate: "Mar 10, 2025",
        },
        {
          Name: "Dr Maria Anderson",
          specialization: "diabetesThyroid",
          bookedDate: "Feb 20, 2025",
        },
        {
          Name: "Dr Lakshmi Reddy",
          specialization: "cardiacHealth",
          bookedDate: "Jan 15, 2025",
        },
        {
          Name: "DR David Joseph",
          specialization: "womenHealth",
          bookedDate: "Feb 5, 2025",
        },
        {
          Name: "Dr Jessica Martinez",
          specialization: "skinHairCare",
          bookedDate: "Mar 1, 2025",
        },
        {
          Name: "Dr Pavan Kumar",
          specialization: "gutDigestive",
          bookedDate: "Jan 20, 2025",
        },
      ];

      // Function to populate the table
      function populateTable(filterSpecialization = "") {
        const tbody = document.querySelector("#dietitianTable tbody");
        tbody.innerHTML = ""; // Clear existing rows

        dietitians.forEach((dietitian) => {
          if (
            filterSpecialization === "" ||
            filterSpecialization === "All" ||
            dietitian.specialization === filterSpecialization
          ) {
            const row = document.createElement("tr");
            row.innerHTML = `
              <td><input type="checkbox"></td>
              <td>${dietitian.Name}</td>
              <td>${getSpecializationLabel(dietitian.specialization)}</td>
              <td>${dietitian.bookedDate}</td>
              <td>
                <button class="icon-btn" title="Chat" onclick="location.href='/chat_user'">
                  <i class="fa-regular fa-message"></i>
                </button>
                <button class="icon-btn" title="Lab Reports" onclick="location.href='/Lab_user'">
                  <i class="fa-solid fa-notes-medical"></i>
                </button>
              </td>
            `;
            tbody.appendChild(row);
          }
        });
      }

      // Function to get the label for the specialization
      function getSpecializationLabel(specialization) {
        switch (specialization) {
          case "weightLossGain":
            return "Weight Loss/Gain Management";
          case "diabetesThyroid":
            return "Diabetes/Thyroid Care Plan";
          case "cardiacHealth":
            return "Cardiac Health";
          case "womenHealth":
            return "Women Health";
          case "skinHairCare":
            return "Skin and Hair Care";
          case "gutDigestive":
            return "Gut/Digestive Diet Plan";
          default:
            return "";
        }
      }

      // Event listener for filter change
      document
        .getElementById("specializationFilter")
        .addEventListener("change", function () {
          populateTable(this.value);
        });

      // Initialize table
      populateTable();
    </script>
